	"main": "index.js",
	"scripts": {
		"prepublish": "safe-publish-latest",
		"pretest": "npm run lint",
		"test": "npm run tests-only",
		"tests-only": "node test",
		"posttest": "npm run audit",
		"coverage": "covert test",
		"lint": "eslint .",
